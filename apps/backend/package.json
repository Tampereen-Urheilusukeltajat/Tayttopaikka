{
  "name": "@tayttopaikka/backend",
  "description": "Backend API for Tayttopaikka",
  "version": "0.0.1",
  "private": true,
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon",
    "start": "ts-node --transpile-only ./src/app.ts",
    "test": "tsx --test",
    "test:coverage": "tsx --test --experimental-test-coverage",
    "test:watch": "tsx --test --test-concurrency=1 --watch",
    "lint": "eslint src --max-warnings 0",
    "format": "prettier -c .",
    "check-types": "tsc --noEmit",
    "db:seed": "knex seed:run",
    "db:seed:make": "knex seed:make"
  },
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/jwt": "^10.0.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.5",
    "@fastify/type-provider-typebox": "^6.1.0",
    "@scaleway/sdk": "^3.1.5",
    "@sinclair/typebox": "^0.34.48",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.7.2",
    "knex": "^3.1.0",
    "mysql": "^2.18.1",
    "redis": "^5.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^25.1.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-love": "^149.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-promise": "^7.2.1",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "prettier-2": "npm:prettier@^3.8.1",
    "tsx": "^4.21.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tampereen-Urheilusukeltajat/Blenderi-Backend.git"
